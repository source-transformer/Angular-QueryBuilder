{
  "compileOnSave": false,
  "compilerOptions": {

    "outDir": "../../out-tsc/lib",
    "declaration": true,
    "inlineSources": true,
    "types": [],
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es5",
      "es6",
      "dom"
    ],



    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
